{
  "extends": [
    "semantic-release-monorepo"
  ],
  "plugins": [
    "@semantic-release/commit-analyzer",
    "@semantic-release/release-notes-generator",
    "@semantic-release/github"
  ],
  "prepare": [
    "@semantic-release/npm"
  ],
  "verifyConditions": [
    {
      "path": "@semantic-release/github",
      "githubUrl": "https://api.github.com"
    }
  ],
  "publish": [
    {
      "path": "@semantic-release/github",
      "githubUrl": "https://api.github.com"
    }
  ],
  "success": false,
  "fail": false,
  "npmPublish": false
}
